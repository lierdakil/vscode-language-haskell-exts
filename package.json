	"contributes": {
		"grammars": [
			{
				"path": "./syntaxes/functions.tmLanguage.json",
				"scopeName": "source.haskell.injection.functions",
				"injectTo": [
					"source.haskell"
				]
			},
			{
				"path": "./syntaxes/operators.tmLanguage.json",
				"scopeName": "source.haskell.injection.operators",
				"injectTo": [
					"source.haskell"
				]
			},
			{
				"path": "./syntaxes/types.tmLanguage.json",
				"scopeName": "source.haskell.injection.types",
				"injectTo": [
					"source.haskell"
				]
			},
			{
				"path": "./syntaxes/ctors.tmLanguage.json",
				"scopeName": "source.haskell.injection.ctors",
				"injectTo": [
					"source.haskell"
				]
			},
			{
				"path": "./syntaxes/charkind.tmLanguage.json",
				"scopeName": "source.haskell.injection.charkind",
				"injectTo": [
					"source.haskell"
				]
			}
		]
	},
